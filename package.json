{
  "name": "workshop-angular2-seed",
  "version": "1.0.0",
  "description": "angular2 starter project based on the offical angular2 seed project",
  "main": "webpack.config.js",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Angular-AfterHours/workshop-angular2-seed.git"
  },
  "devDependencies": {
    "angular2-template-loader": "0.4.0",
    "awesome-typescript-loader": "1.1.1",
    "css-loader": "0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "json-loader": "0.5.4",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "source-map-loader": "0.1.5",
    "to-string-loader": "1.1.4",
    "typescript": "1.8.10",
    "typings": "1.0.4",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-merge": "0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "core-js": "2.4.0",
    "ie-shim": "0.1.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "keywords": [
    "angular2",
    "angular",
    "typescript",
    "javascript",
    "angular2",
    "starter"
  ],
  "author": "Nir Kaufman <nirkaufman@gmail.com> (nirkaufman.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Angular-AfterHours/workshop-angular2-seed/issues"
  },
  "homepage": "https://github.com/Angular-AfterHours/workshop-angular2-seed#readme"
}
